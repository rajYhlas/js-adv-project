{
    "version": 2,
    "installCommand": "npm install",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "buildCommand": "npm run vercel-build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/index.js"
      }
    ]
  }